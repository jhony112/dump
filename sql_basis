 -- Table: public.teams

-- DROP TABLE public.teams;

CREATE TABLE public.teams
(
    team_code character varying COLLATE pg_catalog."default",
    team_name character varying COLLATE pg_catalog."default"
)
WITH (
    OIDS = FALSE
)
TABLESPACE pg_default;

ALTER TABLE public.teams
    OWNER to jhony112;

-- Index: team_code

-- DROP INDEX public.team_code;

CREATE INDEX team_code
    ON public.teams USING btree
    (team_code COLLATE pg_catalog."default")
    TABLESPACE pg_default;



 -- Table: public.support

-- DROP TABLE public.support;

CREATE TABLE public.support
(
    support_id bigint NOT NULL DEFAULT nextval('support_support_id_seq'::regclass),
    support_officer character varying COLLATE pg_catalog."default",
    phone character varying COLLATE pg_catalog."default",
    merchant_id character varying COLLATE pg_catalog."default",
    in_house character varying COLLATE pg_catalog."default",
    phone_2 character varying COLLATE pg_catalog."default",
    visitation character varying COLLATE pg_catalog."default",
    ptsp character varying COLLATE pg_catalog."default",
    CONSTRAINT support_pkey PRIMARY KEY (support_id)
)
WITH (
    OIDS = FALSE
)
TABLESPACE pg_default;

ALTER TABLE public.support
    OWNER to jhony112;



 -- Table: public.regions

-- DROP TABLE public.regions;

CREATE TABLE public.regions
(
    region_id integer NOT NULL DEFAULT nextval('regions_region_id_seq'::regclass),
    region_name character varying COLLATE pg_catalog."default",
    region_head character varying COLLATE pg_catalog."default",
    terminal_id character varying COLLATE pg_catalog."default",
    sbu character varying COLLATE pg_catalog."default",
    merchant_code character varying COLLATE pg_catalog."default"
)
WITH (
    OIDS = FALSE
)
TABLESPACE pg_default;

ALTER TABLE public.regions
    OWNER to jhony112;
