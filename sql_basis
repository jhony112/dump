    tid bigint NOT NULL DEFAULT nextval('pos_transactions_tid_seq'::regclass),
    transaction_id character varying(150) COLLATE pg_catalog."default",
    terminal_id character varying(150) COLLATE pg_catalog."default",
    merchant_id character varying(150) COLLATE pg_catalog."default",
    transaction_type character varying(100) COLLATE pg_catalog."default",
    transaction_date character varying(100) COLLATE pg_catalog."default",
    transaction_amount numeric(15,2),
    "timestamp" bigint,
    CONSTRAINT pos_transactions_pkey PRIMARY KEY (tid)
)
WITH (
    OIDS = FALSE
