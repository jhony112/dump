-- Table: public.account_officer

-- DROP TABLE public.account_officer;

CREATE TABLE public.account_officer
(
    account_officer_id integer NOT NULL DEFAULT nextval('account_officer_account_officer_id_seq'::regclass),
    account_officer_name character varying COLLATE pg_catalog."default",
    terminal_id character varying COLLATE pg_catalog."default",
    CONSTRAINT account_officer_pkey PRIMARY KEY (account_officer_id)
)
WITH (
    OIDS = FALSE
)
TABLESPACE pg_default;

ALTER TABLE public.account_officer
    OWNER to jhony112;

-- Index: account_officer_id

-- DROP INDEX public.account_officer_id;

CREATE INDEX account_officer_id
    ON public.account_officer USING btree
    (account_officer_id)
    TABLESPACE pg_default;






   -- Table: public.pos_transactions

-- DROP TABLE public.pos_transactions;

CREATE TABLE public.pos_transactions
(
    tid bigint NOT NULL DEFAULT nextval('pos_transactions_tid_seq'::regclass),
    transaction_id character varying(150) COLLATE pg_catalog."default",
    terminal_id character varying(150) COLLATE pg_catalog."default",
    merchant_id character varying(150) COLLATE pg_catalog."default",
    transaction_type character varying(100) COLLATE pg_catalog."default",
    transaction_date character varying(100) COLLATE pg_catalog."default",
    transaction_amount numeric(15,2),
    "timestamp" bigint,
    CONSTRAINT pos_transactions_pkey PRIMARY KEY (tid)
)
WITH (
    OIDS = FALSE
)
TABLESPACE pg_default;

ALTER TABLE public.pos_transactions
    OWNER to jhony112;






 -- Table: public.merchant_info

-- DROP TABLE public.merchant_info;

CREATE TABLE public.merchant_info
(
    id bigint NOT NULL DEFAULT nextval('merchant_info_id_seq'::regclass),
    merchant_code_name character varying(200) COLLATE pg_catalog."default",
    merchant_code character varying(150) COLLATE pg_catalog."default",
    sbu character varying(50) COLLATE pg_catalog."default",
    CONSTRAINT merchant_info_pkey PRIMARY KEY (id)
)
WITH (
    OIDS = FALSE
)
TABLESPACE pg_default;

ALTER TABLE public.merchant_info
    OWNER to jhony112;







  -- Table: public.pos_transactions

-- DROP TABLE public.pos_transactions;

CREATE TABLE public.pos_transactions
